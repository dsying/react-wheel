    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.0.0",
    "jest-junit": "^6.3.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.4",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
